{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "types": ["node"],
    "emitDecoratorMetadata": true,
    "target": "es2021",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
